    "editor.tokenColorCustomizations": {
        "[Wal]": {
            "textMateRules": [
                {
                    "settings": {
                        "background": "#0A0907",
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Comment",
                    "scope": [
                        "comment"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#A19C8Eb0"
                    }
                },
                {
                    "name": "String",
                    "scope": [
                        "string",
                        "constant.other.symbol"
                    ],
                    "settings": {
                        "foreground": "#FFC805"
                    }
                },
                {
                    "name": "Regular Expressions and Escape Characters",
                    "scope": [
                        "string.regexp",
                        "constant.character",
                        "constant.other"
                    ],
                    "settings": {
                        "foreground": "#4FFE66"
                    }
                },
                {
                    "name": "Number",
                    "scope": [
                        "constant.numeric"
                    ],
                    "settings": {
                        "foreground": "#2ABAFF"
                    }
                },
                {
                    "name": "Built-in constants",
                    "scope": [
                        "constant.language"
                    ],
                    "settings": {
                        "foreground": "#E48F00"
                    }
                },
                {
                    "name": "Variable",
                    "scope": [
                        "variable"
                    ],
                    "settings": {
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Member Variable",
                    "scope": [
                        "variable.member"
                    ],
                    "settings": {
                        "foreground": "#E48F00"
                    }
                },
                {
                    "name": "Language variable",
                    "scope": [
                        "variable.language"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#FFC805"
                    }
                },
                {
                    "name": "Storage",
                    "scope": [
                        "storage"
                    ],
                    "settings": {
                        "foreground": "#4FFE66"
                    }
                },
                {
                    "name": "type",
                    "scope": [
                        "storage.modifier"
                    ],
                    "settings": {
                        "foreground": "#00FAE8"
                    }
                },
                {
                    "name": "type",
                    "scope": [
                        "meta.specification.type.fortran"
                    ],
                    "settings": {
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Keyword",
                    "scope": [
                        "keyword"
                    ],
                    "settings": {
                        "foreground": "#E48F00"
                    }
                },
                {
                    "name": "Operators",
                    "scope": [
                        "keyword.operator",
                        "keyword.other",
                        "keyword.control",
                        "keyword.logical"
                    ],
                    "settings": {
                        "foreground": "#00FAE8"
                    }
                },
                {
                    "name": "Separators like ; or ,",
                    "scope": [
                        "punctuation.separator",
                        "punctuation.terminator"
                    ],
                    "settings": {
                        "foreground": "#E6E0CBb0"
                    }
                },
                {
                    "name": "Punctuation",
                    "scope": [
                        "punctuation.section"
                    ],
                    "settings": {
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Accessor",
                    "scope": [
                        "punctuation.accessor"
                    ],
                    "settings": {
                        "foreground": "#00FAE8"
                    }
                },
                {
                    "name": "Types fixes",
                    "scope": [
                        "source.java storage.type",
                        "source.haskell storage.type",
                        "source.c storage.type"
                    ],
                    "settings": {
                        "foreground": "#28BDFF"
                    }
                },
                {
                    "name": "Inherited class type",
                    "scope": [
                        "entity.other.inherited-class"
                    ],
                    "settings": {
                        "foreground": "#FFC805"
                    }
                },
                {
                    "name": "Lambda arrow",
                    "scope": [
                        "storage.type.function"
                    ],
                    "settings": {
                        "foreground": "#4FFE66"
                    }
                },
                {
                    "name": "Java primitive variable types",
                    "scope": [
                        "source.java storage.type.primitive"
                    ],
                    "settings": {
                        "foreground": "#FFC805"
                    }
                },
                {
                    "name": "Function name",
                    "scope": [
                        "entity.name.function"
                    ],
                    "settings": {
                        "foreground": "#2ABAFF"
                    }
                },
                {
                    "name": "Function arguments",
                    "scope": [
                        "variable.parameter"
                    ],
                    "settings": {
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Function arguments",
                    "scope": [
                        "meta.parameter"
                    ],
                    "settings": {
                        "foreground": "#E6E0CB"
                    }
                },
                {
                    "name": "Function call",
